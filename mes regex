cat vsftpd | awk '{if( !/^#/ && $3 ~ /^(([1-1][0-9]|[2][0-3])|([0-9]|[2][0-3]))\:([0-5][0-9]|[1][0-2])\-(([1-1][0-9]|[2][0-3])|([0-9]|[2][0-3]))\:([0-5][0-9]|[1][0-2])$/ && $1 == "'$2'") {$3 = "'$3'"};print }' >  outputFile
